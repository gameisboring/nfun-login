    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "pkg": "^5.5.2",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "typescript": "^4.2.4"
